aws_region         = "ap-northeast-1"
project            = "xrypton"
acm_certificate_arn = "arn:aws:acm:ap-northeast-1:123456789012:certificate/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
db_master_username = "xrypton"
db_master_password = "CHANGE_ME_TO_SECURE_PASSWORD"
db_name            = "xrypton"
private_subnet_cidrs = ["10.0.11.0/24", "10.0.12.0/24"]
create_secrets    = true

server_hostname = "xrypton.example.com"

# Set after pushing images to ECR
# api_image = "123456789012.dkr.ecr.ap-northeast-1.amazonaws.com/xrypton-api:latest"
# web_image = "123456789012.dkr.ecr.ap-northeast-1.amazonaws.com/xrypton-web:latest"
# Build web image with: docker build --build-arg NEXT_PUBLIC_SERVER_HOSTNAME=xrypton.example.com -f web.Dockerfile .

# VAPID keys for Web Push notifications
# vapid_public_key            = ""
# vapid_private_key = "BASE64URL_PRIVATE_KEY"

# If create_secrets = false, set these manually:
# database_url_secret_arn      = "arn:aws:secretsmanager:ap-northeast-1:123456789012:secret:xrypton-database-url-xxxxxx"
# vapid_private_key_secret_arn = "arn:aws:secretsmanager:ap-northeast-1:123456789012:secret:xrypton-vapid-private-xxxxxx"
